{
    "name": "panlatent/apple-remote-cli",
    "description": "Apple Remote protocol console application",
    "type": "project",
    "require": {
        "php": ">=5.5",
        "symfony/console": "^3.2",
        "symfony/yaml": "^3.2",
        "guzzlehttp/guzzle": "^6.2",
        "panlatent/daap": "dev-master",
        "panlatent/timer": "dev-master",
        "gabrielrcouto/php-gui": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "panlatent",
            "email": "panlatent@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Panlatent\\AppleRemoteCli\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "bin": [
        "bin/apple-remote"
    ]
}
